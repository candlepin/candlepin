  <build>
    <plugins>
      <plugin>
        <groupId>org.openapitools</groupId>
        <artifactId>openapi-generator-maven-plugin</artifactId>
        <version>4.0.1</version>
        <executions>
          <execution>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <inputSpec>${project.basedir}/candlepin-api-spec.yaml</inputSpec>
              <generatorName>jaxrs-spec</generatorName>
              <configurationFile>${project.basedir}/candlepin-api-config.json</configurationFile>
              <configOptions>
                <interfaceOnly>true</interfaceOnly>
                <generatePom>false</generatePom>
                <dateLibrary>java8</dateLibrary>
                <sourceFolder>src/gen/java/main</sourceFolder>
              </configOptions>
              <templateDirectory>../buildSrc/src/main/resources/templates</templateDirectory>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
